G[n_,p_,q_,r_,s_,m_]:=Sum[HarmonicNumber[j,m]/(((j+r)^p)*((j+s)^q)),{j,1,n}]
V[n_,p_,q_,r_,s_,m_]:=Sum[(HarmonicNumber[j,m]^2)/(((j+r)^p)*((j+s)^q)),{j,1,n}]

(* Lemma H.1 *)
LemH1lhs[n_,m_,r_]:=Sum[1/((j+m)*(j+r)),{j,1,n}]
LemH1rhs[n_,m_,r_]:=If[m<r,((HarmonicNumber[r,1]-HarmonicNumber[m,1])-(HarmonicNumber[n+r,1]-HarmonicNumber[n+m,1]))/(r-m),Infinity]
(* Mathematica does not produce anything in reasonable time 
FullSimplify[LemH1lhs[n,r,m]-LemH1rhs[n,r,m]] 
*)
Clear[n];Clear[m];Clear[r];n=100;m=2;r=3;N[LemH1lhs[n,m,r]-LemH1rhs[n,m,r]]
(* result: 0 *)

(* Lemma H.2 *)
LemH2lhs[n_]:=Sum[1/(j(j+1)),{j,1,n}]
LemH2rhs[n_]:=1-1/(n+1)
Clear[n];FullSimplify[LemH2lhs[n]-LemH2rhs[n]]
(* result: 0 *)
LemH2lhs[Infinity]
(* result: 1 *)
Clear[n];Limit[LemH2rhs[n],{n->Infinity}]
(* result: 1 *)

